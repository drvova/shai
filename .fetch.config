{
  "name": "example",
  "description": "Example agent with MCP fetch server for web scraping and API calls",
  "tools": {
    "builtin": ["*"],
    "builtin_excluded": ["fetch"],
    "mcp": {
      "fetch": {
        "config": {
          "type": "stdio",
          "command": "uvx",
          "args": ["mcp-server-fetch"]
        },
        "enabled_tools": ["*"]
      }
    }
  },
  "temperature": 0.3
}
