{
  "name": "ovh",
  "description": "OVH agent with OVH MCP server for cloud management and API calls",
  "tools": {
    "builtin": ["*"],
    "mcp": {
      "ovh": {
        "config": {
          "type": "http",
          "url": "https://mcp.eu.ovhcloud.com/mcp"
        },
        "enabled_tools": ["*"],
        "excluded_tools": [
          "get-cloud-project-flavor-list"
        ]
      }
    }
  },
  "system_prompt": "{{CODER_BASE_PROMPT}}",
  "max_tokens": 4096,
  "temperature": 0.3
}